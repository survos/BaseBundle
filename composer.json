{
    "name": "survos/base-bundle",
    "description": "Add some tools and resources to a Symfony5 application, including volt theme",
    "type": "symfony-bundle",
    "keywords": ["generator", "skeleton" , "symfony5"],
    "require": {
        "php": ">= 8.0",
        "ext-json": "*",
        "ext-zip": "*",
        "doctrine/common": "^2.0|^3.0",
        "knplabs/knp-menu": "^3.0",
        "knplabs/knp-menu-bundle": "^3.0",
        "knpuniversity/oauth2-client-bundle": "^2.0",
        "mindscreen/yarnlock": "^1.0",
        "symfony/mailer": "*",
        "symfony/string": "^5.0",
        "symfony/webpack-encore-bundle": "^1.6|^2",
        "symfony/yaml": "*",
        "twig/string-extra": "^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "meyfa/php-svg": "^0.9.1",
        "doctrine/doctrine-fixtures-bundle": "^3.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Survos\\BaseBundle\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "symfony": {
            "allow-contrib": "true",
            "id": "base-bundle.survos.com"
        }
    }
}
